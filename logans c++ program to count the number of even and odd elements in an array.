#include<iostream>
#include<math.h>
using namespace std;
int main()
{
    int n,odd=0,even=0,i;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
        cin>>a[i];
    for(int i=0;i<n;i++)
    {
        if (a[i]%2==0)
        {
        even++;
        }
        else
        {
        odd++;
        }
        cout<<"Odd:"<<odd<<endl;
        cout<<"Even:"<<even<<endl;
    }
}
