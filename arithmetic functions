#include<iostream>
#include<math.h>
using namespace std;
int add(int a,int b)
{
   return(a+b);
}
int sub(int a,int b)
{
    return(a-b);
}
int mul(int a,int b)
{
    return(a*b);
}
int divi(int a,int b)
{
    return(a/b);
}
int mod(int a,int b)
{
    return(a%b);
}
int main()
{
    int a,b;
    cin>>a>>b;
    cout<<add(a,b)<<endl;
    cout<<sub(a,b)<<endl;
    cout<<mul(a,b)<<endl;
    cout<<divi(a,b)<<endl;
    cout<<mod(a,b)<<endl;
    
    return 0;
}
